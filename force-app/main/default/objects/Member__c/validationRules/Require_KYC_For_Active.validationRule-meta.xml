<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Require_KYC_For_Active</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
    ISPICKVAL(Status__c, &quot;Active&quot;),
    ISBLANK(KYC_ID__c)
)</errorConditionFormula>
    <errorDisplayField>KYC_ID__c</errorDisplayField>
    <errorMessage>KYC ID is required for Active Members.</errorMessage>
</ValidationRule>
