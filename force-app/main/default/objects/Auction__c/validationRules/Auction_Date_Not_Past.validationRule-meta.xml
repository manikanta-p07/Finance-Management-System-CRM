<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Auction_Date_Not_Past</fullName>
    <active>true</active>
    <errorConditionFormula>Auction_Date__c &lt; TODAY()</errorConditionFormula>
    <errorDisplayField>Auction_Date__c</errorDisplayField>
    <errorMessage>Auction Date must be today or later</errorMessage>
</ValidationRule>
